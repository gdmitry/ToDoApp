{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/jsx-filename-extension": 0
  }
}
